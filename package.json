  "bugs": {
    "url": "https://github.com/dhammikepiyumal/ProShop/issues"
  },
  "homepage": "https://github.com/dhammikepiyumal/ProShop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "mongoose": "^5.12.8"
  },
  "devDependencies": {
    "concurrently": "^6.1.0",
    "nodemon": "^2.0.7"
  }
}
